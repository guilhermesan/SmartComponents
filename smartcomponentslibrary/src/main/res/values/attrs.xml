<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FontTextView">
        <attr name="fontType" format="enum" >
            <enum name="custom" value="0"/>
            <enum name="clear_sans" value="1"/>
            <enum name="clear_sans_bold" value="2"/>
            <enum name="clear_sans_medium" value="3"/>
            <enum name="roboto_black" value="4"/>
            <enum name="roboto_black_italic" value="5"/>
            <enum name="roboto_bold" value="6"/>
            <enum name="roboto_bold_italic" value="7"/>
            <enum name="roboto_italic" value="8"/>
            <enum name="roboto_light" value="9"/>
            <enum name="roboto_light_italic" value="10"/>
            <enum name="roboto_medium" value="11"/>
            <enum name="roboto_medium_italic" value="12"/>
            <enum name="roboto_regular" value="13"/>
            <enum name="roboto_thin" value="14"/>
            <enum name="roboto_thin_italic" value="15"/>
        </attr>
        <attr name="fontPath" format="string"/>
    </declare-styleable>

    <declare-styleable name="FontAwesomeView">
        <attr name="awesomeIcon" format="string"/>
    </declare-styleable>

</resources>